<svg xmlns="http://www.w3.org/2000/svg" width="81.28mm" height="128.5mm" viewBox="0 0 81.28 128.5">
  <defs>
    <linearGradient id="panelGradient" x1="0" x2="0" y1="0" y2="1">
      <stop offset="0" stop-color="#2b2d33" />
      <stop offset="1" stop-color="#1b1d22" />
    </linearGradient>
    <style>
      .title { font-family: 'Roboto', 'Liberation Sans', sans-serif; font-size: 3.8px; fill: #f5f6fa; font-weight: 600; letter-spacing: 0.6px; }
      .label { font-family: 'Roboto', 'Liberation Sans', sans-serif; font-size: 2.8px; fill: #cfd3dc; text-anchor: middle; }
      .sub { font-family: 'Roboto', 'Liberation Sans', sans-serif; font-size: 2.2px; fill: #a7acb7; text-anchor: middle; }
      .jack { font-family: 'Roboto', 'Liberation Sans', sans-serif; font-size: 2.5px; fill: #d9dde5; text-anchor: middle; }
      .divider { stroke: #434750; stroke-width: 0.25; stroke-linecap: round; }
      .step { font-family: 'Roboto Mono', 'Liberation Mono', monospace; font-size: 2.2px; fill: #cfd3dc; font-weight: 500; }
    </style>
  </defs>
  <rect width="81.28" height="128.5" fill="url(#panelGradient)" rx="2" ry="2" />

  <text class="title" x="40.64" y="7">LILITH ADV</text>
  <text class="sub" x="40.64" y="10.5">16-STEP EXPANDER</text>

  <!-- Top controls section -->
  <rect x="1.5" y="11.5" width="78.28" height="7" fill="#202228" stroke="#3a3d46" stroke-width="0.4" rx="1.5" />
  <text class="label" x="8" y="17.8">STEPS</text>
  <text class="label" x="22" y="17.8">GATE</text>
  <text class="label" x="73" y="17.8">RUN</text>

  <!-- Column 1 Headers (Steps 1-8) -->
  <text class="step" x="4" y="24.5">STEP</text>
  <text class="step" x="11" y="24.5">MODE</text>
  <text class="step" x="20" y="24.5">CV</text>
  <text class="step" x="30" y="24.5">GT</text>

  <!-- Column 2 Headers (Steps 9-16) -->
  <text class="step" x="44" y="24.5">STEP</text>
  <text class="step" x="51" y="24.5">MODE</text>
  <text class="step" x="60" y="24.5">CV</text>
  <text class="step" x="70" y="24.5">GT</text>

  <line class="divider" x1="2" y1="25.5" x2="79.28" y2="25.5" />

  <!-- Left Column: Steps 1-8 -->
  <g>
    <text class="step" x="4" y="36.5">1</text>
    <text class="step" x="4" y="46.5">2</text>
    <text class="step" x="4" y="56.5">3</text>
    <text class="step" x="4" y="66.5">4</text>
    <text class="step" x="4" y="76.5">5</text>
    <text class="step" x="4" y="86.5">6</text>
    <text class="step" x="4" y="96.5">7</text>
    <text class="step" x="4" y="106.5">8</text>
  </g>

  <!-- Right Column: Steps 9-16 -->
  <g>
    <text class="step" x="44" y="36.5">9</text>
    <text class="step" x="44" y="46.5">10</text>
    <text class="step" x="44" y="56.5">11</text>
    <text class="step" x="44" y="66.5">12</text>
    <text class="step" x="44" y="76.5">13</text>
    <text class="step" x="44" y="86.5">14</text>
    <text class="step" x="44" y="96.5">15</text>
    <text class="step" x="44" y="106.5">16</text>
  </g>

  <!-- Vertical divider between columns -->
  <line class="divider" x1="40" y1="25.5" x2="40" y2="109" />

  <line class="divider" x1="2" y1="109" x2="79.28" y2="109" />

  <!-- I/O labels -->
  <text class="jack" x="12" y="114">CLK</text>
  <text class="jack" x="26" y="114">RST</text>
  <text class="jack" x="55" y="114">CV</text>
  <text class="jack" x="69" y="114">GATE</text>

  <text class="sub" x="40.64" y="126">SITRI EXPANDER</text>
</svg>
